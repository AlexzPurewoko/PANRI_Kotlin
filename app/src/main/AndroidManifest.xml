<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mizzugi.kensiro.app.panri">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.PanriSettingActivity"
            android:label="Settings"
            android:parentActivityName="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mizzugi.kensiro.app.panri.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.AboutActivity"
            android:label="@string/about_name"
            android:parentActivityName="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mizzugi.kensiro.app.panri.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.GalleryActivity"
            android:label="Gallery"
            android:parentActivityName="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mizzugi.kensiro.app.panri.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.HowToResolveActivity"
            android:label="@string/acthowto_string_title"
            android:parentActivityName="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mizzugi.kensiro.app.panri.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.DiseaseInfo"
            android:label="@string/actinfo_string_title"
            android:parentActivityName="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mizzugi.kensiro.app.panri.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.ImageDiagnose"
            android:label="@string/actimgdiagnose_string_title"
            android:parentActivityName="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mizzugi.kensiro.app.panri.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.StartDiagnose"
            android:label="@string/diagnose_name"
            android:parentActivityName="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mizzugi.kensiro.app.panri.activity.MainActivity" />
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.TutorialFirstUseActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.SAMPLE_CODE" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mizzugi.kensiro.app.panri.activity.SplashScreenActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>